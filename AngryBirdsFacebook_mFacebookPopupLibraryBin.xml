<UIDescription>
	<Popups>
        <!-- TUTORIAL overlay="True" -->
		<Popup_Tutorial name="Popup_Tutorial" fromLibrary="True" alignH="CENTER" alignV="CENTER" width="1024" height="768">
			<Container name="Container_Tutorial" visible="False">
				<Button name="Button_CloseTutorial" MouseUp="CLOSE_TUTORIAL" scaleOnMouseOver="TRUE"/>
			</Container>
	    </Popup_Tutorial>
	    
	    <Popup_FreddieForADay name="FreddieForADayPopup" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER"
			alignV="CENTER">
		</Popup_FreddieForADay>
		
		<Popup_NewSlingShots name="NewSlingShotsPopup" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER"
			alignV="CENTER">
		</Popup_NewSlingShots>
		
		<Popup_LeagueInfo name="PopupLeagueInfo" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER"
			alignV="CENTER">
			
			<Container name="Container_TabContentLeagueInfo" visible="true">
				<MovieClip name="mcLeagueInfoGLASS" />
				<MovieClip name="mcLeagueInfoWOOD" />
				<MovieClip name="mcLeagueInfoSTONE" />
				<MovieClip name="mcLeagueInfoBRONZE" />
				<MovieClip name="mcLeagueInfoSILVER" />
				<MovieClip name="mcLeagueInfoGOLD" />
				<MovieClip name="mcLeagueInfoDIAMOND" />
				<Button name="ButtonLeagueRewardSlingshot1" MouseUp="SLINGSHOT_REWARD_1"/>
				<Button name="ButtonLeagueRewardSlingshot2" MouseUp="SLINGSHOT_REWARD_2"/>
			</Container>
			<Container name="Container_TabContentLeagueProfile" visible="false">
				<MovieClip name="IconLeagueEgg" />
				<MovieClip name="NextLeagueIcon" />
				<MovieClip name="PlayerAvatarBase" />
				<MovieClip name="MC_NotInLeague" />
				<TextField name="TextField_LeagueName" />
	 			<TextField name="TextField_LeagueRating" />
				<TextField name="TextField_PlayerName" />
				<TextField name="TextField_NextLeague" />
				<TextField name="TextField_LeagueProgressValue_Start" />
				<TextField name="TextField_LeagueProgressValue_Goal" />
				<MovieClip name="LeagueProgress" />
 				<Button name="ButtonEditProfile" MouseUp="EDIT_PROFILE" />
			</Container>
		</Popup_LeagueInfo>
		
		<Popup_EditLeagueProfile name="EditLeagueProfile" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER"
			alignV="CENTER">
			<Button name="ButtonConfirm" MouseUp="CONFIRM" />
			<Button name="ButtonBack" MouseUp="BACK" />
			<MovieClip name="Checkbox_FB"/>
			<MovieClip name="ProfileSaving"/>
			<MovieClip name="Title_UseFacebookName" />
			<TextField name="TextField_ErrorMessage" />
			<Container name="Container_PictureSelection">
				<Button name="Slot1" MouseUp="S0" />
				<Button name="Slot2" MouseUp="S1" />
				<Button name="Slot3" MouseUp="S2" />
				<Button name="Slot4" MouseUp="S3" />
				<Button name="Slot5" MouseUp="S4" />
				<Button name="Slot6" MouseUp="S5" />
				<Button name="Slot7" MouseUp="S6" />
				<Button name="Slot8" MouseUp="S7" />
				<Button name="Slot9" MouseUp="S8" />
			</Container>
		</Popup_EditLeagueProfile>
		
		<Popup_LeagueTutorial name="PopupLeagueTutorial" fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER"
			alignV="CENTER">
				<Button name="btnPrevious"/>
				<Button name="btnNext"/>
				<Button name="btnCheckMark"/>
		</Popup_LeagueTutorial>
		
		<Popup_SlingshotRewardPopup name="SlingshotRewardPopup"	fromLibrary="True" overlay="True" width="1024" height="768" alignH="CENTER"
			alignV="CENTER">
			<TextField name="TextField_Title" />
			<TextField name="TextField_Text" />
			<MovieClip name="MovieClip_ImageRef"/>
		</Popup_SlingshotRewardPopup>
		
	    <Popup_QuestionPopup name="QuestionPopup" fromLibrary="True" overlay="True" width="450" height="351" alignH="CENTER"
			alignV="CENTER">
			<TextField name="TextField_Title" />
			<TextField name="TextField_Text" />
			<MovieClip name="Image"/>
		</Popup_QuestionPopup>
	</Popups>
</UIDescription>